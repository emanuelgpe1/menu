local function FBTUYTR_fake_script() -- admin.Put in screen gui 
	local script = Instance.new('LocalScript', admin)

	local player = game.Players.LocalPlayer
	
	local users = {"Um_Aleatorio737",
		"Fernanda_Batatinha45"
	}
	
	local isUser = false
	
	local gui = script.Parent
	
	for i,name in pairs(users) do
		if player.Name == name then
			isUser = true
			game.Players.LocalPlayer:Kick("VocÃª esta na black list ðŸ˜‚")
		end
	end
	
	if not isUser then
		gui.Enabled = true
		wait(0.1)
		print("a")
	end
end
coroutine.wrap(FBTUYTR_fake_script)()
