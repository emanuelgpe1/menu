local function SYZNP_fake_script() -- VGT.Whitelist group 
	local script = Instance.new('LocalScript', VGT)

	
	local groupID = 15047022
	
	
	
	
	local plr = game.Players.LocalPlayer
	
	script.Parent.Enabled = false
	
	repeat wait() until plr.Character
	
	local gui = script.Parent
	
	
	if plr:IsInGroup(groupID) then
	
		script.Parent.Enabled = true
		wait("1")
		game:GetService("StarterGui"):SetCore("SendNotification",{  
			Title = "PETITE HUB",     
			Text = "Bom farm VGT                                    .                                                   Criador: ðŸŒ¹PetiteðŸŒ¹#6547",
			Icon = "rbxassetid://8666601749",
			Duration = 10,
			Color = Success
		})
	
	else
	
		script.Parent:Destroy()
		wait("1")
		game:GetService("StarterGui"):SetCore("SendNotification",{  
			Title = "PETITE HUB",     
			Text = "vocÃª nao e um vgt >:(",
			Icon = "rbxassetid://8666601749",
			Duration = 10,
			Color = Success
		})
	end
end
coroutine.wrap(SYZNP_fake_script)()
local function JXOCSB_fake_script() -- VGT.Put in screen gui 
	local script = Instance.new('LocalScript', VGT)

	local player = game.Players.LocalPlayer
	
	local users = {"Um_Aleatorio737",
		"Fernanda_Batatinha45"
	}
	
	local isUser = false
	
	local gui = script.Parent
	
	for i,name in pairs(users) do
		if player.Name == name then
			isUser = true
			game.Players.LocalPlayer:Kick("VocÃª esta na black list ðŸ˜‚")
		end
	end
	
	if not isUser then
		gui.Enabled = true
		wait(0.1)
		print("a")
	end
end
coroutine.wrap(JXOCSB_fake_script)()
local function IIAT_fake_script() -- TextLabel_2.Script 
	local script = Instance.new('Script', TextLabel_2)

	-- Wretcheddjungle
	while wait(0.5) do
	script.Parent.TextColor3 = Color3.fromHSV(math.random(100)/100, 1, 1)
	end
end
coroutine.wrap(IIAT_fake_script)()
